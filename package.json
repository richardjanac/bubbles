{
  "name": "paddock-bubbles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev",
    "dev:server": "tsx watch --tsconfig tsconfig.server.json server/gameServer.ts",
    "build": "next build && npm run build:server",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "next start",
    "start:server": "node dist/server/index.js",
    "lint": "next lint",
    "postinstall": "npm run build:server"
  },
  "dependencies": {
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.17.13",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.3",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "vercelForceRebuild": "1751999236"
}
